{
  "presets": ["react-app"],
  "plugins": [
    [
      "prismjs",
      {
        "languages": ["javascript", "css", "python", "html"],
        "plugins": [
          "line-numbers",
          "line-highlight",
          "highlight-keywords",
          "normalize-whitespace"
        ],
        "theme": "twilight",
        "css": true
      }
    ]
  ]
}
